{
  "name": "rollup-plugin-posthtml-multi",
  "version": "0.1.0",
  "description": "Better, more flexible, integration between rollup and posthtml.",
  "main": "dist/index.js",
  "module": "dist/module.js",
  "scripts": {
    "test:clean": "rm -rfv test/result/*",
    "test:run": "node test/rollup.test.js",
    "test": "npm-run-all -s 'build:*' 'test:*'",
    "build:clean": "rm -rfv dist/*",
    "build:build": "npx rollup -c",
    "build": "npm-run-all -s 'build:*'"
  },
  "keywords": [
    "rollup-plugin",
    "posthtml",
    "html"
  ],
  "author": "Tomas Ravinskas'",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^8.0.1",
    "posthtml": "^0.11.4",
    "posthtml-match-helper": "^1.0.1",
    "rollup-pluginutils": "^2.8.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "npm-run-all": "^4.1.5",
    "posthtml-extend": "^0.3.0",
    "posthtml-include": "^1.2.0",
    "posthtml-modules": "^0.4.2",
    "rollup": "^1.15.6",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-filesize": "^6.1.1",
    "rollup-plugin-notify": "^1.1.0",
    "rollup-plugin-terser": "^5.0.0"
  }
}
